{"ast":null,"code":"import CartSummary from './CartSummary.vue';\nimport { getCart } from '@/services/cartService';\nexport default {\n  name: 'NavBar',\n  components: {\n    CartSummary\n  },\n  data() {\n    return {\n      cartItems: [],\n      // Initialize as an empty array\n      cartTotal: 0,\n      cartVisible: false // For controlling cart summary visibility\n    };\n  },\n  created() {\n    this.loadCart();\n  },\n  methods: {\n    async loadCart() {\n      try {\n        const response = await getCart('1'); // Replace '1' with actual cart ID if necessary\n        this.cartItems = response.data.items || [];\n        this.cartTotal = response.data.total || 0;\n      } catch (error) {\n        console.error('Error loading cart:', error);\n      }\n    },\n    toggleTheme() {\n      // To do theme logic here\n    },\n    showCartSummary() {\n      this.cartVisible = true;\n    },\n    hideCartSummary() {\n      this.cartVisible = false;\n    }\n  }\n};","map":{"version":3,"names":["CartSummary","getCart","name","components","data","cartItems","cartTotal","cartVisible","created","loadCart","methods","response","items","total","error","console","toggleTheme","showCartSummary","hideCartSummary"],"sources":["C:\\Users\\User\\Documents\\IntelliJ Projects 2024\\comic_city_project\\src\\frontend\\src\\components\\NavBar.vue"],"sourcesContent":["<template>\r\n  <nav class=\"navbar\">\r\n    <div class=\"logo\">COMICITY</div>\r\n    <ul class=\"nav-links\">\r\n      <li><a href=\"#\">Franchises</a></li>\r\n      <li><a href=\"#\">Graphic Novels</a></li>\r\n      <li><a href=\"#\">Manga</a></li>\r\n      <li><a href=\"#\">More</a></li>\r\n    </ul>\r\n\r\n    <div class=\"icon-container\">\r\n      <label for=\"search-input\" class=\"search-label\">\r\n        <input type=\"search\" id=\"search-input\" placeholder=\"Search...\">\r\n        <font-awesome-icon icon=\"search\" class=\"icon1\" />\r\n      </label>\r\n      <div class=\"cart-icon-wrapper\" @mouseover=\"showCartSummary\" @mouseleave=\"hideCartSummary\">\r\n        <font-awesome-icon icon=\"shopping-cart\" class=\"icon cart-icon\" />\r\n        <CartSummary v-if=\"cartVisible\" :cartItems=\"cartItems\" :cartTotal=\"cartTotal\" />\r\n      </div>\r\n\r\n      <font-awesome-icon icon=\"user\" class=\"icon\" />\r\n      <font-awesome-icon icon=\"heart\" class=\"icon\" />\r\n      <font-awesome-icon icon=\"sun\" class=\"icon\" @click=\"toggleTheme\" />\r\n    </div>\r\n  </nav>\r\n</template>\r\n\r\n<script>\r\nimport CartSummary from './CartSummary.vue';\r\nimport { getCart } from '@/services/cartService';\r\n\r\nexport default {\r\n  name: 'NavBar',\r\n  components: {\r\n    CartSummary\r\n  },\r\n  data() {\r\n    return {\r\n      cartItems: [], // Initialize as an empty array\r\n      cartTotal: 0,\r\n      cartVisible: false // For controlling cart summary visibility\r\n    };\r\n  },\r\n  created() {\r\n    this.loadCart();\r\n  },\r\n  methods: {\r\n    async loadCart() {\r\n      try {\r\n        const response = await getCart('1'); // Replace '1' with actual cart ID if necessary\r\n        this.cartItems = response.data.items || [];\r\n        this.cartTotal = response.data.total || 0;\r\n      } catch (error) {\r\n        console.error('Error loading cart:', error);\r\n      }\r\n    },\r\n    toggleTheme() {\r\n      // To do theme logic here\r\n    },\r\n    showCartSummary() {\r\n      this.cartVisible = true;\r\n    },\r\n    hideCartSummary() {\r\n      this.cartVisible = false;\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Navbar Styles */\r\n.navbar {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  padding: 10px 20px;\r\n  background-color: #333;\r\n  color: white;\r\n}\r\n\r\n.logo {\r\n  font-size: 1.5rem;\r\n  font-weight: bold;\r\n}\r\n\r\n.nav-links {\r\n  display: flex;\r\n  list-style: none;\r\n}\r\n\r\n.nav-links li {\r\n  margin: 0 15px;\r\n}\r\n\r\n.nav-links a {\r\n  color: white;\r\n  text-decoration: none;\r\n}\r\n\r\n/* Icon Container */\r\n.icon-container {\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 15px;\r\n  position: relative;\r\n}\r\n\r\n/* Search Input */\r\n.search-label {\r\n  position: relative;\r\n}\r\n\r\ninput[type=\"search\"] {\r\n  background-color: #dddddd;\r\n  width: 0;\r\n  transition: width 0.4s, border-radius 0.4s;\r\n  border-radius: 50%;\r\n  border: 2px solid transparent;\r\n  outline: none;\r\n  padding: 12px 20px;\r\n  color: #dddddd;\r\n  cursor: pointer;\r\n}\r\n\r\ninput[type=\"search\"]:focus {\r\n  border-radius: 20px;\r\n  width: 200px;\r\n  color: black;\r\n}\r\n\r\n/* Cart Icon Wrapper */\r\n.cart-icon-wrapper {\r\n  position: relative;\r\n}\r\n\r\n/* Icon Styles */\r\n.icon1 {\r\n  color: #333;\r\n  position: absolute;\r\n  right: 10px;\r\n  top: 50%;\r\n  transform: translateY(-50%);\r\n  font-size: 1rem;\r\n  transition: opacity 0.4s;\r\n}\r\n\r\ninput[type=\"search\"]:focus + .icon1 {\r\n  opacity: 0;\r\n}\r\n\r\n.icon {\r\n  color: white;\r\n  font-size: 1.5rem;\r\n  cursor: pointer;\r\n}\r\n\r\n.icon:hover {\r\n  color: #888;\r\n}\r\n\r\n/* Mobile responsiveness */\r\n@media (max-width: 768px) {\r\n  .navbar {\r\n    flex-direction: column;\r\n    align-items: flex-start;\r\n  }\r\n\r\n  .nav-links {\r\n    flex-direction: column;\r\n    margin-top: 10px;\r\n  }\r\n\r\n  .nav-links li {\r\n    margin: 5px 0;\r\n  }\r\n\r\n  .icon-container {\r\n    margin-top: 10px;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":"AA4BA,OAAOA,WAAU,MAAO,mBAAmB;AAC3C,SAASC,OAAM,QAAS,wBAAwB;AAEhD,eAAe;EACbC,IAAI,EAAE,QAAQ;EACdC,UAAU,EAAE;IACVH;EACF,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE;MAAE;MACfC,SAAS,EAAE,CAAC;MACZC,WAAW,EAAE,KAAI,CAAE;IACrB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,QAAQ,CAAC,CAAC;EACjB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,QAAQA,CAAA,EAAG;MACf,IAAI;QACF,MAAME,QAAO,GAAI,MAAMV,OAAO,CAAC,GAAG,CAAC,EAAE;QACrC,IAAI,CAACI,SAAQ,GAAIM,QAAQ,CAACP,IAAI,CAACQ,KAAI,IAAK,EAAE;QAC1C,IAAI,CAACN