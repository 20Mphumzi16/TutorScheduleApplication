rgba(0, 0, 0, 0.3);\r\n  padding: 1rem;\r\n  z-index: 1000;\r\n}\r\n\r\n.cart-item {\r\n  display: flex;\r\n  gap: 1rem;\r\n  background-color: #2a3a4b; /* Slightly lighter than the cart summary */\r\n  padding: 0.75rem;\r\n  border-bottom: 1px solid #444; /* Border for separation */\r\n}\r\n\r\n.cart-item:last-child {\r\n  border-bottom: none; /* Remove border for the last item */\r\n}\r\n\r\n.cart-item-image {\r\n  background-size: cover;\r\n  background-position: center;\r\n  width: 70px;\r\n  height: 70px;\r\n  border-radius: 8px;\r\n}\r\n\r\n.cart-item-details {\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n}\r\n\r\n.text-sm {\r\n  color: #a0b3c2; /* Subtle color for small text */\r\n}\r\n\r\n.text-base {\r\n  color: #e0e0e0; /* Lighter color for base text */\r\n}\r\n\r\n.cart-footer {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  padding: 0.75rem 0;\r\n}\r\n\r\n.cart-footer-actions {\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 0.5rem;\r\n}\r\n\r\n.view-cart {\r\n  display: block;\r\n  background-color: #2c3e50; /* Darker background for the button */\r\n  color: white;\r\n  text-align: center;\r\n  font-weight: bold;\r\n  padding: 0.5rem;\r\n  border-radius: 8px;\r\n  text-decoration: none;\r\n}\r\n\r\n.checkout {\r\n  background-color: #3498db; /* Brighter color for the checkout button */\r\n  color: white;\r\n  font-weight: bold;\r\n  padding: 0.5rem;\r\n  border-radius: 8px;\r\n  border: none;\r\n  cursor: pointer;\r\n}\r\n\r\n/* Icon Styles */\r\n.icon1 {\r\n  color: #333;\r\n  position: absolute;\r\n  right: 10px;\r\n  top: 50%;\r\n  transform: translateY(-50%);\r\n  font-size: 1rem;\r\n  transition: opacity 0.4s;\r\n}\r\n\r\ninput[type=\"search\"]:focus + .icon1 {\r\n  opacity: 0;\r\n}\r\n\r\n.icon {\r\n  color: white;\r\n  font-size: 1.5rem;\r\n  cursor: pointer;\r\n}\r\n\r\n.icon:hover {\r\n  color: #888;\r\n}\r\n\r\n/* Mobile responsiveness */\r\n@media (max-width: 768px) {\r\n  .navbar