\r\n      <div>\r\n        <p class=\"text-sm\">Total: ${{ cartTotal }}</p>\r\n        <p class=\"text-sm\">Items: {{ cartItems.length }}</p>\r\n      </div>\r\n      <div class=\"cart-footer-actions\">\r\n        <a href=\"#\" class=\"view-cart\">View Cart</a>\r\n        <button class=\"checkout\">Checkout</button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { getCart } from '@/services/cartService.js';\r\nexport default {\r\n  name: 'CartSummary',\r\n  data() {\r\n    return {\r\n      cartItems: []\r\n    };\r\n  },\r\n  computed: {\r\n    cartTotal() {\r\n      return this.cartItems.reduce((total, item) => {\r\n        return total + item.price * item.quantity;\r\n      }, 0);\r\n    }\r\n  },\r\n  mounted() {\r\n    this.fetchCartItems();\r\n  },\r\n  methods: {\r\n    fetchCartItems() {\r\n\r\n      this.cartItems = [\r\n        {\r\n          id: 1,\r\n          title: 'The Amazing Spider-Man (2018-) #1',\r\n          price: 2.99,\r\n          quantity: 1,\r\n          image: 'https://cdn.usegalileo.ai/stability/6b1f5c61-71c0-48f4-9766-8f4a4b5bb0fa.png'\r\n        },\r\n        {\r\n          id: 2,\r\n          title: 'The Amazing Spider-Man (2018-) #1',\r\n          price: 2.99,\r\n          quantity: 1,\r\n          image: 'https://cdn.usegalileo.ai/stability/e3a7c0c7-2e76-48ff-855a-416f3b0ebb62.png'\r\n        },\r\n        {\r\n          id: 3,\r\n          title: 'The Amazing Spider-Man (2018-) #1',\r\n          price: 2.99,\r\n          quantity: 1,\r\n          image: 'https://cdn.usegalileo.ai/stability/a7891fe8-a129-416d-8d9a-6d59c9714acf.png'\r\n        }\r\n      ];\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Cart Summary styles (as in your previous style section) */\r\n.cart-summary {\r\n  display: none;\r\n  position: absolute;\r\n  right: 0;\r\n  top: 100%;\r\n  width: 300px;\r\n  background-color: #1e2a38;\r\n  border-radius: 8px;\r\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.3);\r\n  padding: 1rem;\r\n  z-index: 1000;\r\n}\r\n\r\n.cart-item {\r\n  display: flex;\r\n  gap: 1rem;\r\n  background-color: #2a3a4b;\r\n  padding: 0.75rem;\r\n  border-bottom: 1px solid #444;\r\n}\r\n\r\n.cart-item:last-child {\r\n  border-bottom: none;\r\n}\r\n\r\n.cart-item-image {\r\n  background-size: cover;\r\n  background-position: center;\r\n  width: 70px;\r\n  height: 70px;\r\n  border-radius: 8px;\r\n}\r\n\r\n.cart-item-details {\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n}\r\n\r\n.text-sm {\r\n  color: #a0b3c2;\r\n}\r\n\r\n.text-base {\r\n  color: #e0e0e0;\r\n}\r\n\r\n.cart-footer {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  padding: 0.75rem 0;\r\n}\r\n\r\n.cart-footer-actions {\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 0.5rem;\r\n}\r\n\r\n.view-cart {\r\n  background-color: #2c3e50;\r\n  color: white;\r\n  text-align: center;\r\n  font-weight: bold;\r\n  padding: 0.5rem;\r\n  border-radius: 8px;\r\n  text-decoration: none;\r\n}\r\n\r\n.checkout {\r\n  background-color: #3498db;\r\n  color: white;\r\n  font-weight: bold;\r\n  padding: 0.5rem;\r\n  border-radius: 8px;\r\n  border: none;\r\n  cursor: pointer;\r\n}\r\n</style>\r\n\r\n"],"mappings":"AA0BA,SAASA,OAAM,QAAS,2BAA2B;AACnD,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,SAASA,CAAA,EAAG;MACV,OAAO,IAAI,CAACF,SAAS,CAACG,MAAM,CAAC,CAACC,KAAK,EAAEC,IAAI,KAAK;QAC5C,OAAOD,KAAI,GAAIC,IAAI,CAACC,KAAI,GAAID,IAAI,CAACE,QAAQ;MAC3C,CAAC,EAAE,CAAC,CAAC;IACP;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,cAAc,CAAC,CAAC;EACvB,CAAC;EACDC,OAAO,EAAE;IACPD,cAAcA,CAAA,EAAG;MAEf,IAAI,CAACT,SAAQ,GAAI,CACf;QACEW,EAAE,EAAE,CAAC;QACLC,KAAK,EAAE,mCAAmC;QAC1CN,KAAK,EAAE,IAAI;QACXC,QAAQ,EAAE,CAAC;QACXM,KAAK,EAAE;MACT,CAAC,EACD;QACEF,EAAE,EAAE,CAAC;QACLC,KAAK,EAAE,mCAAmC;QAC1CN,KAAK,EAAE,IAAI;QACXC,QAAQ,EAAE,CAAC;QACXM,KAAK,EAAE;MACT,CAAC,EACD;QACEF,EAAE,EAAE,CAAC;QACLC,KAAK,EAAE,mCAAmC;QAC1CN,K